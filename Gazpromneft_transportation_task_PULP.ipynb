   "source": [
    "def define_function_to_minimize(matrixes):\n",
    "    \n",
    "    # Set some transitionary variables\n",
    "    volumes = dict()\n",
    "    function = dict()\n",
    "    n_brands = len(matrixes)\n",
    "    \n",
    "    for b in np.arange(0, n_brands):\n",
    "        trans = []\n",
    "        points = (matrixes['matrix_brand_' + str(b)].columns[:-1])\n",
    "        origins = (matrixes['matrix_brand_' + str(b)].index[:-1])\n",
    "    \n",
    "        for p in points:\n",
    "            for o in origins:\n",
    "                string = \"P\" + str(p) + \"O\"+ str(o) + \"_brand_\"+ str(b)\n",
    "                # Add to the dict (key is a string)\n",
    "                function[string] = matrixes['matrix_brand_' + str(b)].loc[o, p]\n",
    "                # Create a variable with a non negative low bound\n",
    "                volumes[string] = pulp.LpVariable(string, lowBound=0)\n",
    "                \n",
    "                \n",
    "    return volumes, function"
   ]
  },
  {
   "cell_type": "markdown",
   "id": "a2f0f399",
   "metadata": {},
   "source": [
    "Function we use to state the problem:"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 18,
   "id": "b4b9f7e6",
   "metadata": {},
   "outputs": [],
   "source": [
    "def state_problem(matrixes, percent_loading):\n",
    "    # Set function's goal\n",
    "    problem = pulp.LpProblem('0', sense=LpMinimize)\n",
    "    \n",
    "    # Specify main function\n",
    "    volumes, function = define_function_to_minimize(matrixes)\n",
    "    \n",
    "    # c conists of the costs only\n",
    "    c = pd.DataFrame(function.items())\n",
    "    c = np.array(c[:][1].values)\n",
    "    \n",
    "    # In v here are the real variables like PnOl_brand_m \n",
    "    v = pd.DataFrame(volumes.items())\n",
    "    v = np.array(v[:][1].values)\n",
    "    \n",
    "    problem+= sum(c * v)\n",
    "    \n",
    "    # Setting a counter for constraints\n",
    "    i = 1\n",
    "    n_brands = len(matrixes)\n",
    "    \n",
    "    # Define restrictions on demand == const\n",
    "    for b in np.arange(0, n_brands):\n",
    "        trans = []\n",
    "        points = (matrixes['matrix_brand_' + str(b)].columns[:-1])\n",
    "        origins = (matrixes['matrix_brand_' + str(b)].index[:-1])\n",
    "    \n",
    "        for p in points:\n",
    "            for o in origins:\n",
    "                string = \"P\" + str(p) + \"O\"+ str(o) + \"_brand_\"+ str(b)\n",
    "                trans.append(volumes.get(string))\n",
    "            \n",
    "            summa = sum(np.array(trans))\n",
    "            # Add a restriction on demand == const\n",
    "            problem += summa == matrixes['matrix_brand_' + str(b)].loc[\"Объем\", p], str(i)\n",
    "            i += 1\n",
    "            trans = []\n",
    "            \n",
    "    # Define restrictions on supply <= upper_const, supply >= lower_const    \n",
    "    for b in np.arange(0, n_brands):\n",
    "        trans = []\n",
    "        points = (matrixes['matrix_brand_' + str(b)].columns[:-1])\n",
    "        origins = (matrixes['matrix_brand_' + str(b)].index[:-1])    \n",
    "    \n",
    "        for o in origins:\n",
    "            for p in points:\n",
    "                string = \"P\" + str(p) + \"O\"+ str(o) + \"_brand_\"+ str(b)\n",
    "                trans.append(volumes.get(string))\n",
    "\n",
    "            summa = sum(np.array(trans))\n",
    "            # Add a restriction on supply <= upper_const   \n",
    "            problem += summa <= matrixes['matrix_brand_' + str(b)].loc[o, \"Объем\"], str(i)\n",
    "            i += 1\n",
    "            \n",
    "            # Add a restriction on supply >= lower_const\n",
    "            const = np.min(percent_loading * matrixes['matrix_brand_' + str(b)].iloc[:-1].loc[:, 'Объем'])\n",
    "            problem += summa >= const, str(i)\n",
    "            i += 1\n",
    "            trans = []\n",
    "    \n",
    "    return problem"
   ]
